{
  "name": "homebrew-loader",
  "version": "0.0.2",
  "description": "",
  "main": "main.js",
  "scripts": {
    "compile": "sass ./src/boot/theme.sass ./src/boot/dist/theme.css && electron-builder -wl ",
    "compile-x86": "sass ./src/boot/theme.sass ./src/boot/dist/theme.css && electron-builder -wl --ia32",
    "compile-all": "sass ./src/boot/theme.sass ./src/boot/dist/theme.css && electron-builder -mwl",
    "build": "sass ./src/boot/theme.sass ./src/boot/dist/theme.css",
    "test": "sass ./src/boot/theme.sass ./src/boot/dist/theme.css && electron . --no-sandbox"
  },
  "keywords": [
    "homebrew",
    "boot",
    "pwed"
  ],
